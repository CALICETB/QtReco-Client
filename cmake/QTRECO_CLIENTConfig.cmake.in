# QtReco_ClientConfig.cmake.in
#
# cmake config file for DCACHE_GUI
#  QTRECO_CLIENT_INCLUDE_DIRS  - the DCACHE_GUI include directory
#  QTRECO_CLIENT_LIBRARIES     - The libraries needed to use DCACHE_GUI
#  QTRECO_CLIENT_DEFINITIONS   - the compile time definitions


#fixme: not true if INCLUDE_INSTALL_DIR is absolute path
SET( QTRECO_CLIENT_INCLUDE_DIRS "@FULL_INCLUDE_INSTALL_DIR@" )
SET( QTRECO_CLIENT_INCLUDE_DIRS "${QTRECO_CLIENT_INCLUDE_DIRS}")

SET( QTRECO_CLIENT_LIBRARIES "@FULL_LIB_INSTALL_DIR@/@FULL_LIBRARY_NAME@" )

SET( QTRECO_CLIENT_DEFINITIONS @QTRECO_CLIENT_EXPORT_DEFINITIONS@ )

SET( QTRECO_CLIENT_ROOT_DIR "@CMAKE_INSTALL_PREFIX@" )

SET( QTRECO_CLIENT_MAJOR_VERSION "@QTRECO_CLIENT_MAJOR_VERSION@" )
SET( QTRECO_CLIENT_MINOR_VERSION "@QTRECO_CLIENT_MINOR_VERSION@" )
SET( QTRECO_CLIENT_PATCH_LEVEL    "@QTRECO_CLIENT_PATCH_LEVEL@"    )


SET( QTRECO_CLIENT_FOUND TRUE )