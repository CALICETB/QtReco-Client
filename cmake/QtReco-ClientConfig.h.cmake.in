#ifndef QtReco-ClientConfig_H
#define QtReco-ClientConfig_H

// version macros
#define QtReco-Client_MAJOR_VERSION @QtReco-Client_VERSION_MAJOR@
#define QtReco-Client_MINOR_VERSION @QtReco-Client_VERSION_MINOR@
#define QtReco-Client_PATCH_LEVEL @QtReco-Client_VERSION_PATCH@
#define QtReco-Client_VERSION_STR "@QtReco-Client_VERSION_MAJOR@.@QtReco-Client_VERSION_MINOR@.@QtReco-Client_VERSION_PATCH@"
#define QtReco-Client_VERSION_GE( MAJV , MINV , PLEV )  \
     ( (QtReco-Client_MAJOR_VERSION > MAJV) || ( (QtReco-Client_MAJOR_VERSION == MAJV) && (QtReco-Client_MINOR_VERSION > MINV) ) \
  || ( (QtReco-Client_MAJOR_VERSION == MAJV) && (QtReco-Client_MINOR_VERSION == MINV) && (QtReco-Client_PATCH_LEVEL >= PLEV) ) )

// project source dir
// useful for icons
#define QtReco-Client_DIR "@CMAKE_INSTALL_PREFIX@"

#endif // QtReco-ClientConfig_H